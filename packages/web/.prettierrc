{
	"trailingComma": "es5",
	"tabWidth": 4,
	"semi": false,
	"printWidth": 120,
	"singleQuote": true,
	"plugins": [
		"@trivago/prettier-plugin-sort-imports"
	],
	"importOrder": [
		"^solid-js",
		"^@(.*)$",
		"^../",
		"^./"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"overrides": [
		{
			"files": "*.yml",
			"options": {
				"tabWidth": 2
			}
		}
	]
}